#include "main.h"

/**
 * _strncpy - copies a string
 * @dest: string copied into
 * @src: string copied to
 * @n: number of times a string is copied
 * Return: pointer to dest
 */
char *_strncpy(char *dest, char *src, int n)
{
int i;

for (i = 0; dest[i] != '\0'; i++)
{
dest[i] = src[i];
}
for (i = 0; i < n; i++)
{
dest[i] = '\0';
}
return (dest);
}
